package myLoopClass2;

public class MyIfBloack {

	public static void main(String[] args) {
		String variable = "even";
		for (int i = 0; i <= 20; i++) {
			if (i > 0) {
				if ((i % 2 == 0) && variable.equalsIgnoreCase("even")) {
					System.out.println(i);
				}else {
					System.out.println(i);
				}
			}
		}

	}

}
